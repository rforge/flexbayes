\name{start.posterior}
\alias{start}
\alias{start.posterior}
\alias{end}
\alias{end.posterior}
\title{The starting and ending iterations for a posterior object}

\usage{start(x)
end(x)}

\arguments{
\item{x}{A \code{posterior} object}
}

\description{
	Returns the index of the first or last saved iteration of the samples in a \code{posterior} object.
}
\seealso{
   \code{\link{nvar.posterior}},
   \code{\link{thin.posterior}},
   \code{\link{varnames.posterior}},
   \code{\link{window.posterior}}.
}

