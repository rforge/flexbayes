\name{thin.posterior}
\alias{thin}
\alias{thin.posterior}
\title{The amount of thinning in a posterior object}

\usage{thin(x)}

\arguments{
\item{x}{A \code{posterior} object}
}

\description{
	Returns the amount of thinning (interval between saved samples) 
	in a \code{posterior} object.  
}
\seealso{
   \code{\link{nvar.posterior}},
   \code{\link{subset.posterior}},
   \code{\link{varnames.posterior}},
   \code{\link{window.posterior}}.
}
